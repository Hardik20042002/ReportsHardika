<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://phpcoder.tech/multiselect/js/jquery.multiselect.js"></script>
    <link rel="stylesheet" href="https://phpcoder.tech/multiselect/css/jquery.multiselect.css">
	<style>
		.popup {
			display: none;
			position: fixed;
			top: 48%;
			left: 50%;
			transform: translate(-50%, -50%);
			width: 96%;
			height: 88%;
			padding: 20px;
			background-color: #ffffff;
			border: 1px solid #cccccc;
			box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.5);
			z-index: 999;
            position: relative;
            overflow: auto;
		}
		#close {
			position: relative;
			top: 20px;
			left: 20px;
			cursor: pointer;
		}
        #download{
            position:relative;
            left: 20px;
            top: 20px;
        }
	</style>
</head>
<body onload="openPopup()">
    <button id="download" class="btn btn-lg btn-outline-dark" onClick="export_data()">Download excel</button> 
    <button id="close" class="btn btn-lg btn-outline-dark" onclick="location.href='<%=baseurl%>/salestock'">Close</button> 
	<div class="popup " id="myPopup">
            <%if(isColor=="No"){%>
                    <table class="table" id="data" style="margin-top: 15px;">
                        <thead class="bg-dark" style="color: #ffffff;">
                            <tr>
                                <th scope="col" rowspan="2">PrevMonth Sale</th>
                                <th scope="col" rowspan="2">FOS</th>
                                <th scope="col" rowspan="2">CLASS</th>
                                <th scope="col" rowspan="2">Dealer Name</th>
                                <%for(var i=0;i<model.length;i++){%>
                                    <% var a=model[i].indexOf(' ');var x=model[i].indexOf('(');var y=model[i].indexOf(')');%>
                                    <%if(type=="sast"){%>
                                        <th scope="col" colspan="2"><%= model[i].substring(a+1,x)%> <%= model[i].substring(x+1,y)%></th>
                                    <%}else{%>
                                        <th scope="col"><%= model[i].substring(a+1,x)%> <%= model[i].substring(x+1,y)%></th>
                                    <%}%>
                                <%}%>
                                <%if(model.length>1){%>
                                    <th scope="col" colspan="2">Total</th>
                                <%}%>
                            </tr>
                            <tr>
                                <%if(type=="sast"){%>
                                    <%for(var i=0;i<model.length;i++){%>
                                        <%if(stockstr[i].length<2){%>
                                            <th scope="col" colspan="2">0</th>
                                        <%}else{%>
                                            <th scope="col" colspan="2"><%= stockstr[i]%></th>
                                        <%}%>
                                    <%}%>
                                <%}else if(type=="sa"){%>
                                    <%for(var i=0;i<model.length;i++){%>
                                        <%if(stockstr[i].length<2){%>
                                            <th scope="col">0</th>
                                        <%}else{%>
                                            <th scope="col"><%= stockstr[i]%></th>
                                        <%}%>
                                    <%}%>
                                <%}else{%>
                                    <%for(var i=0;i<model.length;i++){%>
                                        <%if(stockstr[i].length<2){%>
                                            <th scope="col">0</th>
                                        <%}else{%>
                                            <th scope="col"><%= stockstr[i]%></th>
                                        <%}%>
                                    <%}%>
                                <%}%>
                                <%if(model.length>1){%>
                                    <%if(type=="sast"||type=="sa"){%>
                                        <th scope="col">Sale</th>
                                        <th scope="col">Stock</th>
                                    <%}else{%>
                                        <th scope="col">Stock</th>
                                        <th scope="col">Sale</th>
                                    <%}%>
                                <%}%>
                            </tr>
                        </thead>
                        <tbody>
                            <%var cnt1=0%>
                            <%var cnt2=0%>
                            <%for(var i=0;i<dealer.length;i++){%>
                                <%if(!selectedfos.includes(fos[i].split(' ')[1])){continue;}%>
                                <tr scope="row">
                                    <th><%= fos[i].split(' ')[0]%></th>
                                    <th><%= fos[i].split(' ')[1]%></th>
                                    <th><%= fos[i].split(' ')[2]%></th>
                                    <th><%= dealer[i].split('!')[0]%></th>
                                    <%if(type=="sast"){%>
                                        <%for(var j=0;j<model.length;j++){%>
                                            <th><%= sale[i*model.length+j]%></th>
                                            <th><%= stock[i*model.length+j]%></th>
                                        <%}%>
                                    <%}else if(type=="sa"){%>
                                        <%for(var j=0;j<model.length;j++){%>
                                            <th><%= sale[i*model.length+j]%></th>
                                        <%}%>
                                    <%}else{%>
                                        <%for(var j=0;j<model.length;j++){%>
                                            <th><%= stock[i*model.length+j]%></th>
                                        <%}%>
                                    <%}%>
                                    <%if(model.length>1){%>
                                        <%if(type=="sast"||type=="sa"){%>
                                            <%cnt1+=tsale[i]%>
                                            <%cnt2+=tstock[i]%>
                                            <th><%= tsale[i]%></th>
                                            <th><%= tstock[i]%></th>
                                        <%}else{%>
                                            <%cnt1+=tsale[i]%>
                                            <%cnt2+=tstock[i]%>
                                            <th><%= tstock[i]%></th>
                                            <th><%= tsale[i]%></th>
                                        <%}%>
                                    <%}%>
                                </tr>
                            <%}%>
                            <tr scope="row">
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <%if(type=="sast"){%>
                                    <%for(var j=0;j<model.length;j++){%>
                                        <th></th>
                                        <th></th>
                                    <%}%>
                                <%}else if(type=="sa"){%>
                                    <%for(var j=0;j<model.length;j++){%>
                                        <th></th>
                                    <%}%>
                                <%}else{%>
                                    <%for(var j=0;j<model.length;j++){%>
                                        <th></th>
                                    <%}%>
                                <%}%>
                                <%if(model.length>1){%>
                                    <%if(type=="sast"||type=="sa"){%>
                                        <th><%= cnt1%></th>
                                        <th><%= cnt2%></th>
                                    <%}else{%>
                                        <th><%= cnt2%></th>
                                        <th><%= cnt1%></th>
                                    <%}%>
                                <%}%>
                            </tr>
                        </tbody>
                    </table>
                <%}else{%>
                    <table class="table" id="data">
                        <thead class="bg-primary">
                            <tr>
                                <th scope="col" rowspan="3">PrevMonth Sale</th>
                                <th scope="col" rowspan="3">FOS</th>
                                <th scope="col" rowspan="3">CLASS</th>
                                <th scope="col" rowspan="3">Dealer Name</th>
                                <%for(var i=0;i<model.length;i++){%>
                                    <%for(var j=0;j<colors.length;j++){%>
                                        <%if(!distColors[i].includes(colors[j])){continue;}%>
                                        <% var a=model[i].indexOf(' ');var x=model[i].indexOf('(');var y=model[i].indexOf(')');%>
                                        <%if(type=="sast"){%>
                                            <th scope="col" colspan="2"><%= model[i].substring(a+1,x)%> <%= model[i].substring(x+1,y)%></th>
                                        <%}else{%>
                                            <th scope="col"><%= model[i].substring(a+1,x)%> <%= model[i].substring(x+1,y)%></th>
                                        <%}%>
                                    <%}%>
                                <%}%>
                                <th scope="col" rowspan="2" colspan="2">Total</th>
                            </tr>
                            <tr>
                                <%for(var i=0;i<model.length;i++){%>
                                    <%for(var j=0;j<colors.length;j++){%>
                                        <%if(!distColors[i].includes(colors[j])){continue;}%>
                                        <%if(type=="sast"){%>
                                            <th scope="col" colspan="2"><%= colors[j]%></th>
                                        <%}else{%>
                                            <th scope="col"><%= colors[j]%></th>
                                        <%}%>
                                    <%}%>
                                <%}%>
                            </tr>
                            <tr>
                                <%for(var i=0;i<model.length;i++){%>
                                    <%for(var j=0;j<colors.length;j++){%>
                                        <%if(!distColors[i].includes(colors[j])){continue;}%>
                                        <%if(type=="sast"){%>
                                            <%if(stockstr[i].length<2){%>
                                                <th scope="col" colspan="2">0</th>
                                            <%}else{%>
                                                <th scope="col" colspan="2"><%= stockstr[i]%></th>
                                            <%}%>
                                        <%}else if(type=="sa"){%>
                                            <%if(stockstr[i].length<2){%>
                                                <th scope="col">0</th>
                                            <%}else{%>
                                                <th scope="col"><%= stockstr[i]%></th>
                                            <%}%>
                                        <%}else{%>
                                            <%if(stockstr[i].length<2){%>
                                                <th scope="col">0</th>
                                            <%}else{%>
                                                <th scope="col"><%= stockstr[i]%></th>
                                            <%}%>
                                        <%}%>
                                    <%}%>
                                <%}%>
                                <%if(type=="sast"||type=="sa"){%>
                                    <th scope="col">Sale</th>
                                    <th scope="col">Stock</th>
                                <%}else{%>
                                    <th scope="col">Stock</th>
                                    <th scope="col">Sale</th>
                                <%}%>
                            </tr>
                        </thead>
                        <tbody>
                            <%var itr=0%>
                            <%var cnt1=0%>
                            <%var cnt2=0%>
                            <%for(var i=0;i<dealer.length;i++){%>
                                <%if(!selectedfos.includes(fos[i].split(' ')[1])){itr+=model.length*colors.length;continue;}%>
                                <tr scope="row">
                                    <th><%= fos[i].split(' ')[0]%></th>
                                    <th><%= fos[i].split(' ')[1]%></th>
                                    <th><%= fos[i].split(' ')[2]%></th>
                                    <th><%= dealer[i].split('!')[0]%></th>
                                    <%if(type=="sast"){%>
                                        <%for(var j=0;j<model.length;j++){%>
                                            <%for(var k=0;k<colors.length;k++){%>
                                                <%if(!distColors[j].includes(colors[k])){itr+=1;continue;}%>
                                                <th><%= sale[itr]%></th>
                                                <th><%= stock[itr]%></th>
                                                <%itr+=1%>
                                            <%}%>
                                        <%}%>
                                    <%}else if(type=="sa"){%>
                                        <%for(var j=0;j<model.length;j++){%>
                                            <%for(var k=0;k<colors.length;k++){%>
                                                <%if(!distColors[j].includes(colors[k])){itr+=1;continue;}%>
                                                <th><%= sale[itr]%></th>
                                                <%itr+=1%>
                                            <%}%>
                                        <%}%>
                                    <%}else{%>
                                        <%for(var j=0;j<model.length;j++){%>
                                            <%for(var k=0;k<colors.length;k++){%>
                                                <%if(!distColors[j].includes(colors[k])){itr+=1;continue;}%>
                                                <th><%= stock[itr]%></th>
                                                <%itr+=1%>
                                            <%}%>
                                        <%}%>
                                    <%}%>
                                    <%if(type=="sast"||type=="sa"){%>
                                        <%cnt1+=tsale[i]%>
                                        <%cnt2+=tstock[i]%>
                                        <th><%= tsale[i]%></th>
                                        <th><%= tstock[i]%></th>
                                    <%}else{%>
                                        <%cnt1+=tsale[i]%>
                                        <%cnt2+=tstock[i]%>
                                        <th><%= tstock[i]%></th>
                                        <th><%= tsale[i]%></th>
                                    <%}%>
                                </tr>
                            <%}%>
                            <tr scope="row">
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <%if(type=="sast"){%>
                                    <%for(var j=0;j<model.length;j++){%>
                                        <%for(var k=0;k<colors.length;k++){%>
                                            <%if(!distColors[j].includes(colors[k])){continue;}%>
                                            <th></th>
                                            <th></th>
                                        <%}%>
                                    <%}%>
                                <%}else if(type=="sa"){%>
                                    <%for(var j=0;j<model.length;j++){%>
                                        <%for(var k=0;k<colors.length;k++){%>
                                            <%if(!distColors[j].includes(colors[k])){continue;}%>
                                            <th></th>
                                        <%}%>
                                    <%}%>
                                <%}else{%>
                                    <%for(var j=0;j<model.length;j++){%>
                                        <%for(var k=0;k<colors.length;k++){%>
                                            <%if(!distColors[j].includes(colors[k])){continue;}%>
                                            <th></th>
                                        <%}%>
                                    <%}%>
                                <%}%>
                                <%if(type=="sast"||type=="sa"){%>
                                    <th><%= cnt1%></th>
                                    <th><%= cnt2%></th>
                                <%}else{%>
                                    <th><%= cnt2%></th>
                                    <th><%= cnt1%></th>
                                <%}%>
                            </tr>
                        </tbody>
                    </table>
                <%}%>
	</div>
    <script src="<%=link%>"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
	<script>
        function export_data(){
                            let data=document.getElementById('data');
                            var fp=XLSX.utils.table_to_book(data,{sheet:'sale_stock_report'});
                            XLSX.write(fp,{
                                bookType:'xlsx',
                                type:'base64'
                            });
                            XLSX.writeFile(fp,'orderSheet.xlsx');
                        }
		function openPopup() {
			document.getElementById("myPopup").style.display = "block";
		}
	</script>

</body>
</html>
